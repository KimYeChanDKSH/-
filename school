#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a[10];
    int i, k, j;
    printf("정수 10가지 입력>");
    for(i = 0; i < 10; i++)
    {
        scanf("%d", &a[i]);
    }
    k = a[0];
    for(i = 1; i < 10; i++)
    {
        if(k < a[i])
        {
            k = a[i];
        }
    }
    j = a[0];
     for(i = 1; i < 10; i++)
    {
        if(j > a[i])
        {
            j = a[i];
        }
    }
    printf("%d\n", k);
    printf("%d\n", j);
}
